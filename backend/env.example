app_name="Compression Tool API"
environment="development"
database_url="sqlite:///./app.db"
paddle_public_key=""
paddle_env="sandbox"
file_ttl_hours=6
cors_origins=["http://localhost:5173"]
storage_dir="storage"
free_mode_enabled=true
# Set an ISO 8601 timestamp to automatically end free mode, e.g. 2025-12-31T23:59:59Z
free_mode_until="2026-01-31T23:59:59Z"
enforce_payment=false
free_download_limit=3
client_trial_enabled=true
allowed_mime_types=["application/pdf","image/jpeg","image/png"]
max_input_file_mb=50
max_total_input_mb=200
enforce_page_limits=true


